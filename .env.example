# Environment variables for API keys and secrets
# Copy this file to .env and fill in your actual values

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Twilio credentials
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here

# ElevenLabs API Key
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Telegram Bot Tokens
TELEGRAM_BOT_TOKEN_VERONICA=your_telegram_bot_token_veronica_here
TELEGRAM_BOT_TOKEN_JASMINE=your_telegram_bot_token_jasmine_here
TELEGRAM_BOT_TOKEN_ANI=your_telegram_bot_token_ani_here

# Bubble IDs
BUBBLE_ID_VERONICA=your_bubble_id_veronica_here
BUBBLE_ID_JASMINE=your_bubble_id_jasmine_here
BUBBLE_ID_ANI=your_bubble_id_ani_here

# Voice IDs for ElevenLabs
VOICE_ID_VERONICA=your_voice_id_veronica_here
VOICE_ID_JASMINE=your_voice_id_jasmine_here
VOICE_ID_ANI=your_voice_id_ani_here
